{
  "name": "jumpthon",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "deploy:trigger": "npx trigger.dev@3.3.17 deploy",
    "dev": "concurrently --kill-others --names \"next,trig\" --prefix-colors \"yellow,blue\" \"pnpm run dev:next\" \"pnpm run dev:trigger\"",
    "dev:next": "next dev --turbo",
    "dev:trigger": "pnpm dlx trigger.dev@3.3.17 dev",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "knip": "knip"
  },
  "dependencies": {
