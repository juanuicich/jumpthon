    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.40.0",
    "geist": "^1.3.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
    "lucide-react": "^0.479.0",
    "next": "^15.2.1",
    "next-auth": "5.0.0-beta.25",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
