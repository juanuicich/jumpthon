{
  "name": "jumpthon",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "deploy:trigger-prod": "npx trigger.dev@3.3.17 deploy",
    "dev": "concurrently --kill-others --names \"next,trig\" --prefix-colors \"yellow,blue\" \"next dev --turbo\" \"pnpm run dev:trigger\"",
    "dev:trigger": "pnpm dlx trigger.dev@3.3.17 dev",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.1.20",
    "@auth/drizzle-adapter": "^1.8.0",
