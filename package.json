    "ai": "^4.1.54",
    "chromium-bidi": "^2.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "geist": "^1.3.1",
    "google-auth-library": "^9.15.1",
    "googleapis": "^146.0.0",
